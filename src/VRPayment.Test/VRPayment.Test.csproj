<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <Nullable>annotations</Nullable>
  </PropertyGroup>
  <PropertyGroup>
    <AssemblyName>VRPayment.Test</AssemblyName>
    <AssemblyTitle>VR Payment SDK for C#</AssemblyTitle>
    <Authors>VR Payment GmbH</Authors>
    <AutoGenerateBindingRedirects>true</AutoGenerateBindingRedirects>
    <Company>VR Payment GmbH</Company>
    <Copyright>Apache-2.0</Copyright>
    <Description>VR Payment SDK for C#</Description>
    <GenerateBindingRedirectsOutputType>true</GenerateBindingRedirectsOutputType>
    <GenerateRuntimeConfigurationFiles>true</GenerateRuntimeConfigurationFiles>
    <IsPackable>false</IsPackable>
    <OutputType>Library</OutputType>
    <PackageId>VRPayment.Test</PackageId>
    <RootNamespace>VRPayment.Test</RootNamespace>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.NET.Test.Sdk" Version="17.14.1" />
    <PackageReference Include="NUnit" Version="4.3.2" />
    <PackageReference Include="NUnit3TestAdapter" Version="5.0.0" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\VRPayment\VRPayment.csproj" />
  </ItemGroup>
</Project>
